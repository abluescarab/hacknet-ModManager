<UserControl x:Class="HacknetModManager.PopupButtons" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:HacknetModManager"
        mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
        <Button x:Name="OkayButton" Content="OK" Width="75" Height="25" Click="OkayButton_Clicked"
                x:FieldModifier="private" />
        <Button x:Name="CancelButton" Content="Cancel" Width="75" Height="25" Click="ButtonCancel_Click"
                x:FieldModifier="private" />
    </StackPanel>
</UserControl>
